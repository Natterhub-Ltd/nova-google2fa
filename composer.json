{
    "name": "383project/nova-google2fa",
    "description": "This package provides Google2FA to Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "bacon/bacon-qr-code": "^3.0",
        "pragmarx/google2fa": "^8.0",
        "pragmarx/google2fa-laravel": "^2.1",
        "pragmarx/recovery": "^0.2.0"
    },
    "require-dev": {
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Project383\\Google2fa\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Project383\\Google2fa\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
